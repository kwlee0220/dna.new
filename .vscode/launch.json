    "version": "0.2.0",
    "configurations": [
        { "name": "Node",
            "type": "python", "request": "launch", "console": "integratedTerminal",
            "env": { "PYTHONPATH": "${workspaceFolder}" },
            "program": "${workspaceFolder}/scripts/dna_node.py",
            "args": [
                "conf/etri_051.yaml",
            ],
            "justMyCode": true
        },
        { "name": "Track Video",
            "type": "python", "request": "launch", "console": "integratedTerminal",
            "env": { "PYTHONPATH": "${workspaceFolder}" },
            "program": "${workspaceFolder}/scripts/dna_node_track.py",
            "args": [
                "conf/etri_041.yaml",
            ],
            "justMyCode": true
        },
        { "name": "Detect Video",
            "type": "python", "request": "launch", "console": "integratedTerminal",
            "env": { "PYTHONPATH": "${workspaceFolder}" },
            "program": "${workspaceFolder}/scripts/dna_node_detect.py",
            "args": [
                "conf/etri_051.yaml",
                // "--detector", "dna.detect.yolov4",
                "--detector", "dna.detect.rcnn50_fpn",
                // "--detector", "dna.detect.yolov5:model=m&score=0.45",
                // "--detector", "file:${env:HOME}/Dropbox/data/etri/etri_051_det.txt",
                // "--output", "C:/Temp/data/cam_1_test.txt",
            ],
            "justMyCode": true
        },
        { "name": "Show Video",
            "type": "python", "request": "launch", "console": "integratedTerminal",
            "env": { "PYTHONPATH": "${workspaceFolder}" },
            "program": "${workspaceFolder}/scripts/dna_node_show.py",
            "args": [
                "conf/etri_051.yaml",
            ],
            "justMyCode": true
        },
        { "name": "Test",
            "type": "python", "request": "launch", "console": "integratedTerminal",
            "env": { "PYTHONPATH": "${workspaceFolder}" },
            "program": "${workspaceFolder}/test.py",
            "args": [
                "${workspaceFolder}/conf/etri_051.yaml:camera",
                // "C:/Users/kwlee/Dropbox/data/etri/etri_051.mp4",
                // "conf/etri_live_04.yaml:camera",
                "--no_sync",
            ],
            "justMyCode": true
        },
        { "name": "Publish",
            "type": "python", "request": "launch", "console": "integratedTerminal",
            "env": { "PYTHONPATH": "${workspaceFolder}" },
            "program": "${workspaceFolder}/scripts/dna_publish_events.py",
            "args": [
                "${workspaceFolder}/track_output_etri_051.csv",
            ],
            "justMyCode": true
        },
    ]
}